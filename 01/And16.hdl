/**
 * 16-bit bitwise And:
 * for i = 0..15: out[i] = (a[i] and b[i])
 */

CHIP And16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
    Nand(a=a[0], b=b[0], out=o0);
    Nand(a=o0, b=o0, out=out[0]);

    Nand(a=a[1], b=b[1], out=o1);
    Nand(a=o1, b=o1, out=out[1]);

    Nand(a=a[2], b=b[2], out=o2);
    Nand(a=o2, b=o2, out=out[2]);

    Nand(a=a[3], b=b[3], out=o3);
    Nand(a=o3, b=o3, out=out[3]);
    
    Nand(a=a[4], b=b[4], out=o4);
    Nand(a=o4, b=o4, out=out[4]);

    Nand(a=a[5], b=b[5], out=o5);
    Nand(a=o5, b=o5, out=out[5]);

    Nand(a=a[6], b=b[6], out=o6);
    Nand(a=o6, b=o6, out=out[6]);

    Nand(a=a[7], b=b[7], out=o7);
    Nand(a=o7, b=o7, out=out[7]);

    Nand(a=a[8], b=b[8], out=o8);
    Nand(a=o8, b=o8, out=out[8]);

    Nand(a=a[9], b=b[9], out=o9);
    Nand(a=o9, b=o9, out=out[9]);

    Nand(a=a[10], b=b[10], out=o10);
    Nand(a=o10, b=o10, out=out[10]);

    Nand(a=a[11], b=b[11], out=o11);
    Nand(a=o11, b=o11, out=out[11]);
    
    Nand(a=a[12], b=b[12], out=o12);
    Nand(a=o12, b=o12, out=out[12]);

    Nand(a=a[13], b=b[13], out=o13);
    Nand(a=o13, b=o13, out=out[13]);

    Nand(a=a[14], b=b[14], out=o14);
    Nand(a=o14, b=o14, out=out[14]);

    Nand(a=a[15], b=b[15], out=o15);
    Nand(a=o15, b=o15, out=out[15]);
    
}